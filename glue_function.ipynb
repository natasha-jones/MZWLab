    "def glue(L):\n",
    "    s = 0\n",
    "    length = len(L) - 1\n",
    "    for number in L:\n",
    "        s += number * (10 ** length) # assigns value based on position (tens, thousands, etc.)\n",
    "        length -= 1\n",
    "    return s"
   ]
